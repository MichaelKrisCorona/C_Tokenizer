CFLAGS = -g -O3


# first target is built by default

all: tmain



#include

tokenizer.o: tokenizer.h

history.o: history.h

tmain.o: tmain.c


# $@=target, $^=prerequisites

tmain: tokenizer.o history.o tmain.o

	cc -o $@ $^ $(CFLAGS) $(OBJS)


$(OBJS): history.h tokenizer.h


#deletes files generated by compilation

clean:

	rm -f *.o tmain *~ *#


run:tmain

	./tmain
